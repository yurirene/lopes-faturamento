<input type="text" id="preco" value="3" >
<input type="text" id="qtd" >
<input type="text" id="total" >
<script>

$(document).ready(function(){

    $('#qtd').on('change', function(){
        var qtd = parseint( $('#qtd').val() );
        var preco = parsefloat( $('#preco').val() );
        
        var total = qtd * preco;

        $('#total').val(total);
    });


});


</script>